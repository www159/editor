{
    // "extends": "../../tsconfig.json",
    "include": ["./**/*.ts", "./**/*.tsx"],
    "exclude": ["node_module"],
    "compilerOptions": {
        "lib": ["ES2019", "DOM"],
        "importHelpers": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "jsx": "react-jsx",
        "target": "ES5",
        "esModuleInterop": true,
        "paths": {
            "@editor": ["./src"],
            "@editor/*": ["./src/*"],
        }
    }
}